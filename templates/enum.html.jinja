<h5 class="title is-5">
  <a href="#enum-{{ enum.full_name }}" id="enum-{{ enum.full_name }}">Enum <code title="{{ enum.full_name }}">{{ enum.name }}</code></a>
</h5>
<p>{{ description }}</p>
<!-- <h6 class="title is-6">Values</h6> -->
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    {% for enum_value in enum.values %}
    <tr>
      <td><code>{{ enum_value.name }}</code></td>
      <td>{{ enum_value.number }}</td>
      <td>{{ enum_value.description }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
