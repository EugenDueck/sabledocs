<h5 class="title is-5" id="service-{{ service.full_name }}">
  <a href="#service-{{ service.full_name }}">service <code title="{{ service.full_name }}">{{ service.name }}</code></a>
</h5>
<div class="block">
  <p>{{ description }}</p>
</div>

{% for method in service.methods %}
<h6 class="title is-6">rpc <code>{{ method.name }} ({{ method.request_type }}) -> {{ method.response_type }}</code></h6>
{% endfor %}
